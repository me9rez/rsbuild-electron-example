{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "outputs": [
                "dist/**",
                "dist-electron/**"
            ],
            "dependsOn": [
                "^build"
            ]
        },
        "dev:render": {
            "persistent": true,
            "cache": false
        },
        "dev:main": {
            "persistent": true,
            "cache": false
        }
    }
}